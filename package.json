{
  "name": "@oswld/nuxt-firebase",
  "version": "0.3.1",
  "description": "Firebase module for Nuxt",
  "repository": "kai-oswald/nuxt-firebase",
  "homepage": "https://kai-oswald.github.io/nuxt-firebase/",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
